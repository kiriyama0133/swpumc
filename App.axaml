<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:suki="clr-namespace:SukiUI;assembly=SukiUI"
             xmlns:sukiUi="clr-namespace:SukiUI.Controls;assembly=SukiUI"
             xmlns:materialIcons="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
             x:Class="swpumc.App"
			 xmlns:themes="clr-namespace:Material.Styles.Themes;assembly=Material.Styles"
             xmlns:local="using:swpumc"
             RequestedThemeVariant="Dark">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
        <Application.Resources>
            <ResourceDictionary>
                <FontFamily x:Key="JetBrainsMono">/Assets/JetBrainsMono-Regular.ttf#JetBrains Mono</FontFamily>
                <FontFamily x:Key="Minecraft">/Assets/Minecraft.ttf#Minecraft</FontFamily>
            </ResourceDictionary>
        </Application.Resources>

    <Application.Styles>
        <FluentTheme />
        <suki:SukiTheme />
        <materialIcons:MaterialIconStyles />
    </Application.Styles>
</Application>